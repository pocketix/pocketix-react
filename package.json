{
  "name": "pocketix-react-monorepo",
  "version": "1.0.0",
  "scripts": {
	"build:lib": "npm run build --workspace packages/pocketix-editor",
	"build:demo": "npm run build --workspace demo",
	"start:demo": "npm run start --workspace demo",
	"install:all": "npm ci --maxsockets 1 --workspace packages/pocketix-editor && npm ci --maxsockets 1 --workspace demo && npm ci --maxsockets 1"
  },
  "author": "Petr John",
  "license": "MIT",
  "bugs": {
	"url": "https://github.com/pocketix/pocketix-react/issues"
  },
  "homepage": "https://github.com/pocketix/pocketix-react#readme",
  "workspaces": ["packages/*", "demo"]
}
